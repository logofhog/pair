<h4>Nearby Users</h4>
<% for user in @main_page.nearby %>
  <li>  <%= link_to user.name, user_path(user), :remote=> true %>
      (<%= user.distance.round(2) %> miles)
      <%= link_to 'message', new_message_path(:receiver => user), :remote=> true %>
      <div id = 'new_message<%= user.id %>'></div>
  </li>
  <% end %>
